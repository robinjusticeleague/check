{
	"$schema": "../../../../../../packages/@checkjs/check/configuration_schema.json",
	"linter": {
		"rules": {
			"style": {
				"useNamingConvention": {
					"level": "error",
					"options": {
                        "conventions": [
							{
								"selector": {
									"kind": "const"
								},
								"match": "(.*?)$",
								"formats": ["camelCase"]
							}
                        ]
					}
				}
			}
		}
	}
}

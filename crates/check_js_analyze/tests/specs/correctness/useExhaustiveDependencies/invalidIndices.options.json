{
    "$schema": "../../../../../../packages/@checkjs/check/configuration_schema.json",
    "linter": {
        "rules": {
            "correctness": {
                "useExhaustiveDependencies": {
                    "level": "error",
                    "options": {
                        "hooks": [
                            {
                                "name": "useMyEffect",
                                "closureIndex": 0,
                                "dependenciesIndex": 0,
                                "stableResult": [
                                    -1
                                ]
                            }
                        ]
                    }
                }
            }
        }
    }
}
